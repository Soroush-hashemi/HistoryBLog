@using Microsoft.AspNetCore.Razor.TagHelpers;
@using TestBlog.Web.Areas.Admin.Models.Posts;
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"]</title>
</head>
<div>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Panels/dist/css/bootstrap-theme.css">
    <link rel="stylesheet" href="~/Panels/dist/css/rtl.css">
    <link rel="stylesheet" href="~/Panels/bower_components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Panels/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Panels/dist/css/AdminLTE.css">
    <link rel="stylesheet" href="~/Panels/dist/css/skins/_all-skins.min.css">
    <link rel="stylesheet" href="~/https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <link rel="stylesheet" href="~/Panels/dist/css/customize-adminlte.css">
</div>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <partial name="_Header" />
        <partial name="_SideBar" />
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <footer class="main-footer text-left">
            <strong>
                Copyleft &copy; <a href=""> nobody </a>
            </strong>
        </footer>
        <partial name="_AsideBar" />
        <div class="control-sidebar-bg"></div>
    </div>
    <script src="~/Panels/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Panels/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Panels/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Panels/bower_components/fastclick/lib/fastclick.js"></script>
    <script src="~/Panels/dist/js/adminlte.min.js"></script>
    <script src="~/Panels/dist/js/demo.js"></script>
    <script src="~/js/custom.js"></script>
    <script src="~/js/global.js"></script>
    <script src="~//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <partial name="_ValidationScriptsPartial" />
    <script>
        $(document).ready(function() {
            $('.sidebar-menu').tree();
        })
    </script>
    @await RenderSectionAsync("Scripts", false)
</body>
</html>